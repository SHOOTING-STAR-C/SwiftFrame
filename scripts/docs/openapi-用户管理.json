      "ResetPasswordRequest": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "verificationCode": {
            "type": "string"
          },
          "newPassword": {
            "type": "string",
            "maxLength": 30,
            "minLength": 8,
            "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[@$!%*?&#_+])[A-Za-z\\d@$!%*?&#_+]+$"
          }
        },
        "required": [
