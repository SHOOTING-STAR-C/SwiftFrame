server:
  port: 8081
  servlet:
    context-path: /swift
spring:
  mail:
    host: ${MAIL_HOST:smtp.example.com}
    port: ${MAIL_PORT:587}
    username: ${MAIL_USERNAME:your_email@example.com}
    password: ${MAIL_PASSWORD:your_password}
  sql:
    init:
      mode: never
  datasource:
    druid:
      master:
        url: jdbc:mysql://${DB_HOST:localhost}:3306/swift?characterEncoding=utf8&serverTimezone=UTC
        driver-class-name: com.mysql.cj.jdbc.Driver
        username: ${DB_USERNAME:root}
        password: ${DB_PASSWORD:}
        validation-query: SELECT 1 from dual
        filters: stat,wall,slf4j
        mapper-locations: classpath*:sqlmapper/mysql/*.xml
        initialize-schema: true
        sql-scripts: classpath:sql/mysql/schema.sql,classpath:sql/mysql/init-security-data.sql
      pg:
        url: jdbc:postgresql://${PG_HOST:localhost}:5432/swift_bus
        driver-class-name: org.postgresql.Driver
        username: ${PG_USERNAME:postgres}
        password: ${PG_PASSWORD:}
        validation-query: SELECT 1
        filters: stat,wall,slf4j
        mapper-locations: classpath*:sqlmapper/pg/*.xml
        initialize-schema: true
        sql-scripts: classpath:sql/postgresql/schema.sql,classpath:sql/postgresql/ai-schema.sql,classpath:sql/postgresql/data.sql
  security:
    jwt:
      secret: ${JWT_SECRET:your_jwt_secret_key_change_this_in_production}
      expiration: 86400000
      refresh-expiration: 2592000000
    account:
      maxLoginAttempts: 5
      lockDurationMinutes: 30
      passwordExpiryDays: 30
  data:
    redis:
      host: ${REDIS_HOST:localhost}
      port: ${REDIS_PORT:6379}
      password: ${REDIS_PASSWORD:}
      database: 0
logging:
  level:
    root: info
security:
  auth:
    white-list: /auth/login,/auth/register,/auth/publicKey,/auth/forgot-password/**,/auth/refresh,/v3/api-docs/**,/swagger-ui/**,/swagger-ui.html,/ai/chat/anonymous,GET:/ai/models/**,/error
crypto:
  aes:
    key: ${AES_KEY:your_aes_key_256_bits_long_change_this}
  rsa:
    public-key: ${RSA_PUBLIC_KEY:}
    private-key: ${RSA_PRIVATE_KEY:}
